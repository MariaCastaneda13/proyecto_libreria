<div>
  <h1>Welcome {{name}} to The Galactic Bookshelf </h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Description</th> 
      <th>Photo</th>
      <th>Price</th>
      <th>Code</th>
      <th>Stock</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>